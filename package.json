{
  "name": "disklet",
  "version": "0.1.1",
  "description": "A tiny, composable filesystem API",
  "author": "Airbitz, Inc.",
  "contributors": [
    "William Swanson <william@airbitz.co>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/disklet.git"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.js",
  "types": "src/index.d.ts",
  "files": [
    "changelog.md",
    "lib/*",
    "src/index.d.ts"
  ],
  "dependencies": {
    "rfc4648": "^0.9.1"
  },
  "devDependencies": {
    "husky": "^0.13.3",
    "mocha": "^3.3.0",
    "reify": "^0.8.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "source-map-support": "^0.4.14",
    "standard": "^10.0.2",
    "tmp": "^0.0.31"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "standard '*.js' 'src/**/*.js'",
    "precommit": "npm run lint",
    "prepublish": "npm run lint && npm run build",
    "test": "rollup -c && mocha --require reify --require source-map-support/register test/test.js"
  },
  "browser": {
    "fs": null,
    "path": null,
    "tmp": null
  }
}
